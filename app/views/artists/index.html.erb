<h2>All Artists</h2>

<div class="container">
  <div class="col-md-2">
    <ul class="list-group">
    <% @artists.each do |artist| %>
      <%= link_to artist.name, artist, class:"list-group-item" %>
    <% end %>
    </ul>
    <%= link_to "New Artist", new_artist_path, class:"btn btn-primary"%>
  </div>
</div>